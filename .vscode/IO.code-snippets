                  "template <typename _T, const _T _mod>",
                  "struct modint",
                  "{",
                  "\t_T x;",
                  "\tmodint(_T _x = 0) : x(_x) {}",
                  "\tinline modint operator+() const",
                  "\t{",
                  "\t\treturn *this;",
                  "\t}",
                  "\tinline modint operator-() const",
                  "\t{",
                  "\t\treturn !x ? 0 : _mod - x;",
                  "\t}",
                  "\tinline modint &operator++()",
                  "\t{",
                  "\t\t++x;",
                  "\t\tif (x >= _mod)",
                  "\t\t\tx -= _mod;",
                  "\t\treturn *this;",
                  "\t}",
                  "\tinline modint &operator--()",
                  "\t{",
                  "\t\t--x;",
                  "\t\tif (x < 0)",
                  "\t\t\tx += _mod;",
                  "\t\treturn *this;",
                  "\t}",
                  "\tinline modint operator++(_T)",
                  "\t{",
                  "\t\tint res = x;",
                  "\t\tif (x >= _mod)",
                  "\t\t\tx -= _mod;",
                  "\t\treturn res;",
