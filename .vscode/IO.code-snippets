                  "\t{",
                  "\t\tx = (_T)((ll)x * rhs.x % _mod);",
                  "\t\treturn *this;",
                  "\t}",
                  "\ttemplate <typename _G>",
                  "\tinline modint operator^(_G rhs) const",
                  "\t{",
                  "\t\tmodint a = x, res = 1;",
                  "\t\twhile (rhs)",
                  "\t\t{",
                  "\t\t\tif (rhs & 1)",
                  "\t\t\t\tres *= a;",
                  "\t\t\ta *= a;",
                  "\t\t\trhs >>= 1;",
                  "\t\t}",
                  "\t\treturn res;",
                  "\t}",
                  "\tinline modint inv()",
                  "\t{",
                  "\t\treturn this->pow(_mod - 2);",
                  "\t}",
                  "\tinline modint operator/(const modint &rhs) const",
                  "\t{",
                  "\t\treturn (*this) * rhs.inv();",
