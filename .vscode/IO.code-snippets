                  "\t\tres += rhs.x;",
                  "\t\tif (res >= _mod)",
                  "\t\t\tres -= _mod;",
                  "\t\treturn res;",
                  "\t}",
                  "\tinline modint operator-(const modint &rhs) const",
                  "\t{",
                  "\t\t_T res = x;",
                  "\t\tres -= rhs.x;",
                  "\t\tif (res < 0)",
                  "\t\t\tres += _mod;",
                  "\t\treturn res;",
                  "\t}",
                  "\tinline modint operator*(const modint &rhs) const",
                  "\t{",
                  "\t\treturn (_T)((ll)x * rhs.x % _mod);",
